{
  "name": "dymantic/multilingual-posts",
  "description": "Laravel Model and helpers for multilingual posts",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Joyner",
      "email": "michael@dymanticdesign.com"
    }
  ],
  "require": {
    "cviebrock/eloquent-sluggable": "^10.0",
    "spatie/laravel-translatable": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Dymantic\\MultilingualPosts\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dymantic\\MultilingualPosts\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dymantic\\MultilingualPosts\\MultilingualPostsServiceProvider"
      ]
    }
  }
}
